{
    "name": "aporat/store-receipt-validator",
    "description": "PHP receipt validator for Apple iTunes, Google Play and Amazon App Store",
    "type": "library",
    "minimum-stability": "dev",
    "repositories": [
      {
        "type": "package",
        "package": {
          "name": "zhangwei-bi/store-receipt-validator",
          "version": "1.0.0",
          "type": "package",
          "source": {
            "url": "https://github.com/zhangwei-bi/store-receipt-validator",
            "type": "git",
            "reference": "master"
          }
        }
      }
    ],
    "authors": [
        {
            "name": "Adar Porat",
            "email": "adar.porat@gmail.com",
            "homepage": "https://github.com/aporat"
        },
        {
            "name": "Stanimir Kukudov",
            "email": "stanimir.kukudov@gmail.com",
            "homepage": "https://github.com/stanimir-kukudov"
        }
    ],
    "keywords": [
        "itunes",
        "apple",
        "in app",
        "purchase",
        "receipt"
    ],
    "homepage": "https://github.com/aporat/store-receipt-validator",
    "license": [
        "Apache-2.0"
    ],
    "require": {
        "php": "^7.3|^8.0.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "nesbot/carbon": "^1.0|^2.0",
        "google/apiclient": "^2.10",
        "google/apiclient-services": "~0.249",
        "robrichards/xmlseclibs": "^3.0.4",
        "zhangwei-bi/store-receipt-validator": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "ReceiptValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReceiptValidator\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/aporat/store-receipt-validator/issues"
    }
}
